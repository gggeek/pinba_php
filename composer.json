{
    "name": "gggeek/polyfill-pinba",
    "description": "A pure-php implementation of the API exposed by the native Pinba extension",
    "license": "GPL-2.0",
    "provide": {
        "ext-pinba": "*"
    },
    "require": {
        "php": "^5.3.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0 || ^8.5.14",
        "phpunit/phpunit-selenium": "*",
        "yoast/phpunit-polyfills": "*"
    },
    "autoload": {
        "psr-4": {
            "PinbaPhp\\Polyfill\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    }
}
